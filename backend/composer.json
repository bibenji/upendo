{
    "name": "upendo/upendo",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "src/AppKernel.php",
            "src/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        },
        "files": [ "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php" ]
    },
    "require": {
        "php": ">=7.1",
        "symfony/symfony": "~3.3",
        "api-platform/core": "^2.1@beta",
        "api-platform/api-platform": "v2.1.0-beta.2",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "doctrine/doctrine-migrations-bundle": "~1.2",
        "symfony/swiftmailer-bundle": "^2.4.10",
        "symfony/monolog-bundle": "^3.0.2",
        "symfony/polyfill-apcu": "^1.0",
        "sensio/distribution-bundle": "^5.0.19",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "^2.1",
        "dunglas/action-bundle": "^0.3",
        "nelmio/cors-bundle": "^1.4",
        "ramsey/uuid": "^3.6",
        "hwi/oauth-bundle": "^0.5.3",
        "nekland/woketo": "~1.1",
        "predis/predis": "~1.1",
        "snc/redis-bundle": "~2.0",
        "swarrot/swarrot-bundle": "~1.4"
    },
    "require-dev": {
        "api-platform/schema-generator": "^1.2",
        "behat/behat": "^3.1",
        "behat/symfony2-extension": "^2.1",
        "behat/mink": "^1.7",
        "behat/mink-extension": "^2.2",
        "behat/mink-browserkit-driver": "^1.3.1",
        "behatch/contexts": "^2.5",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "friendsofphp/php-cs-fixer": "^1.12",
        "fzaninotto/faker": "^1.6",
        "phpunit/phpunit":"~6.0",
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0",
        "willdurand/faker-bundle": "^1.3"
    },
    "scripts": {
        "symfony-scripts": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "7.1"
        }
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative"
    }
}
