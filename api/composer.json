{
    "name": "upendo/api",
    "license": "proprietary",
    "type": "project",
    "description": "Upendo Api",
    "homepage": "https://www.upendo.com",
    "authors": [
        {
            "name": "Mlanawo Mbechezi",
            "email": "mlanawo@mbechezi.com",
            "homepage": "https://mlanawo.mbechezi.com"
        },
        {
            "name": "Benjamin Billette",
            "email": "Benjamin.Billet@hotmail.com",
            "homepage": "https://benjamin.billette.com"
        }
    ],
    "autoload": {
        "psr-4": { "Upendo\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Upendo\\Tests\\": "tests/" },
        "files": [ "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php" ]
    },
    "require": {
        "php": ">=7.1",
        "symfony/symfony": "~4.0",
        "api-platform/core": "~2.1",
        "doctrine/orm": "^2.6",
        "doctrine/doctrine-bundle": "^1.8",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "doctrine/doctrine-migrations-bundle": "~1.3",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/monolog-bundle": "^3.1",
        "sensio/framework-extra-bundle": "^5.1",
        "nelmio/cors-bundle": "^1.5",
        "phpdocumentor/reflection-docblock": "^3.3",
        "twig/twig": "^2.4",
        "ramsey/uuid": "^3.7",
        "snc/redis-bundle": "~2.0",
        "predis/predis": "~1.1"
    },
    "require-dev": {
        "behat/behat": "^3.4",
        "behat/mink": "^1.7-dev",
        "behat/mink-browserkit-driver": "^1.3.2-dev",
        "behat/mink-extension": "^2.3",
        "behat/symfony2-extension": "^2.1",
        "behatch/contexts": "^2.8",
        "doctrine/doctrine-fixtures-bundle": "^2.4",
        "fzaninotto/faker": "^1.7",
        "phpstan/phpstan": "^0.9.1",
        "phpunit/phpunit": "~6.5",
        "symfony/phpunit-bridge": "^4.0"
    },
    "config": {
        "platform": {
            "php": "7.1.8"
        },
        "sort-packages": true,
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "public",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative"
    }
}
