{
    "name": "upendo/api",
    "license": "proprietary",
    "type": "project",
    "description": "Upendo Api",
    "homepage": "https://upendo.com",
    "authors": [
        {
            "name": "Mlanawo Mbechezi",
            "email": "mlanawo@mbechezi.com",
            "homepage": "https://mlanawo.mbechezi.com"
        },
        {
            "name": "Benjamin Billette",
            "email": "Benjamin.Billet@hotmail.com"
        }
    ],
    "autoload": {
        "psr-4": { "Upendo\\": "src/" },
        "classmap": [ "src/AppKernel.php", "src/AppCache.php" ]

    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" },
        "files": [ "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php" ]
    },
    "require": {
        "php": ">=7.1",
        "symfony/symfony": "~4.0",
        "api-platform/core": "~2.1",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "doctrine/doctrine-migrations-bundle": "~1.2",
        "symfony/swiftmailer-bundle": "^2.6",
        "symfony/monolog-bundle": "^3.1",
        "sensio/distribution-bundle": "^5.0.20",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "^2.1",
        "dunglas/action-bundle": "^0.3",
        "nelmio/cors-bundle": "^1.5",
        "phpdocumentor/reflection-docblock": "^3.1",
        "twig/twig": "^2.0",
        "ramsey/uuid": "^3.6",
        "snc/redis-bundle": "~2.0",
        "predis/predis": "~1.1",
        "swarrot/swarrot-bundle": "~1.4"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.3",
        "behat/behat": "^3.3",
        "behat/symfony2-extension": "^2.1",
        "behat/mink": "^1.7",
        "behat/mink-extension": "^2.2",
        "behat/mink-browserkit-driver": "^1.3.2",
        "behatch/contexts": "^2.7",
		"fzaninotto/faker": "^1.6",
		"willdurand/faker-bundle": "^1.3",
        "doctrine/doctrine-fixtures-bundle": "^2.3"
    },
    "scripts": {
        "symfony-scripts": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "platform": {
            "php": "7.1.8"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
